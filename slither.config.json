{
  "detectors_to_exclude": "naming-convention|solc-version",
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "disable_color": false,
  "filter_paths": "@openzeppelin/contracts|contracts/itf|contracts/mocks",
  "ignore_compile": true,
  "legacy_ast": false,
  "buidler_cache_directory": "build/cache",
  "hardhat_artifacts_directory": "build/artifacts",
  "solc_remaps": "@openzeppelin/contracts=node_modules/@openzeppelin/contracts"
}
